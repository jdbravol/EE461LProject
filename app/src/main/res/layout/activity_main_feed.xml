<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:layout_editor_absoluteY="25dp"
    tools:layout_editor_absoluteX="0dp">


    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="360dp"
        android:layout_height="149dp"
        app:srcCompat="@drawable/ut1"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        tools:layout_editor_absoluteY="-14dp"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:fontFamily="sans-serif-smallcaps"
        android:text="Occupato"
        android:textColor="@android:color/background_light"
        android:textSize="50sp"
        tools:layout_editor_absoluteY="59dp"
        tools:layout_editor_absoluteX="67dp" />

    <TabHost
        android:layout_width="374dp"
        android:layout_height="424dp"
        tools:layout_editor_absoluteX="-8dp"
        android:id="@+id/tabHost3"
        tools:layout_editor_absoluteY="135dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TabWidget
                android:id="@android:id/tabs"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <FrameLayout
                android:id="@android:id/tabcontent"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <android.support.constraint.ConstraintLayout
                    android:id="@+id/SEARCH"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    tools:layout_editor_absoluteY="0dp"
                    tools:layout_editor_absoluteX="0dp">

                    <TextView
                        android:id="@+id/textView17"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="sans-serif-smallcaps"
                        android:text="Org. Name:"
                        android:textColor="@android:color/white"
                        android:textSize="24sp"
                        app:layout_constraintBaseline_toBaselineOf="@+id/orgName"
                        app:layout_constraintRight_toLeftOf="@+id/orgName"
                        android:layout_marginRight="8dp"
                        android:layout_marginLeft="8dp"
                        app:layout_constraintLeft_toLeftOf="parent" />

                    <EditText
                        android:id="@+id/orgName"
                        android:layout_width="192dp"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="16dp"
                        android:layout_marginStart="8dp"
                        android:ems="10"
                        android:inputType="textPersonName"
                        android:layout_marginTop="8dp"
                        app:layout_constraintTop_toBottomOf="@+id/textView12"
                        android:layout_marginRight="8dp"
                        app:layout_constraintRight_toRightOf="parent" />

                    <Button
                        android:id="@+id/nameGo"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="16dp"
                        android:text="Go"
                        android:layout_marginTop="8dp"
                        app:layout_constraintTop_toBottomOf="@+id/orgName"
                        android:layout_marginRight="16dp"
                        app:layout_constraintRight_toRightOf="parent" />

                    <TextView
                        android:id="@+id/textView12"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="16dp"
                        android:fontFamily="sans-serif-smallcaps"
                        android:text="Search By Organization"
                        android:textColor="@color/burntOrange"
                        android:textSize="24sp"
                        tools:layout_editor_absoluteX="57dp"
                        app:layout_constraintTop_toTopOf="parent"
                        android:layout_marginTop="8dp" />

                    <TextView
                        android:id="@+id/textView13"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="sans-serif-smallcaps"
                        android:text="Search By Everything Else"
                        android:textColor="@color/burntOrange"
                        android:textSize="24sp"
                        tools:layout_editor_absoluteX="39dp"
                        android:layout_marginTop="8dp"
                        app:layout_constraintTop_toBottomOf="@+id/nameGo" />

                    <EditText
                        android:id="@+id/editText7"
                        android:layout_width="197dp"
                        android:layout_height="42dp"
                        android:layout_marginEnd="16dp"
                        android:ems="10"
                        android:inputType="textPersonName"
                        android:layout_marginTop="8dp"
                        app:layout_constraintTop_toBottomOf="@+id/textView13"
                        android:layout_marginRight="8dp"
                        app:layout_constraintRight_toRightOf="parent" />

                    <TextView
                        android:id="@+id/textView19"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="sans-serif-smallcaps"
                        android:text="Category:"
                        android:textColor="@android:color/white"
                        android:textSize="24sp"
                        app:layout_constraintBaseline_toBaselineOf="@+id/editText7"
                        app:layout_constraintRight_toLeftOf="@+id/editText7"
                        android:layout_marginRight="8dp"
                        android:layout_marginLeft="8dp"
                        app:layout_constraintLeft_toLeftOf="parent" />

                    <TextView
                        android:id="@+id/textView20"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="sans-serif-smallcaps"
                        android:text="Date:"
                        android:textColor="@android:color/white"
                        android:textSize="24sp"
                        app:layout_constraintBaseline_toBaselineOf="@+id/editText9"
                        android:layout_marginLeft="0dp"
                        app:layout_constraintLeft_toLeftOf="@+id/textView19" />

                    <CheckBox
                        android:id="@+id/freeFood"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="8dp"
                        android:layout_marginTop="25dp"
                        android:fontFamily="sans-serif-smallcaps"
                        android:text="Free Food"
                        android:textColor="@android:color/white"
                        android:textSize="24sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/textView20"
                        app:layout_constraintVertical_bias="0.0"
                        android:layout_marginLeft="0dp"
                        app:layout_constraintLeft_toLeftOf="@+id/textView20" />

                    <EditText
                        android:id="@+id/editText9"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="16dp"
                        android:ems="10"
                        android:inputType="date"
                        app:layout_constraintRight_toRightOf="parent"
                        android:layout_marginTop="8dp"
                        app:layout_constraintTop_toBottomOf="@+id/editText7" />

                    <Button
                        android:id="@+id/button14"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="16dp"
                        android:text="Go"
                        app:layout_constraintBottom_toBottomOf="parent"
                        android:layout_marginRight="16dp"
                        app:layout_constraintRight_toRightOf="parent"
                        android:layout_marginTop="8dp"
                        app:layout_constraintTop_toBottomOf="@+id/editText9" />

                </android.support.constraint.ConstraintLayout>

                <LinearLayout
                    android:id="@+id/MAIN"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <GridLayout
                        android:id="@+id/featuredEventsTab"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                    </GridLayout>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/SUBSCRIBED"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <GridLayout
                        android:id="@+id/subscribedEventsTab"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                    </GridLayout>
                </LinearLayout>
            </FrameLayout>
        </LinearLayout>
    </TabHost>

</android.support.constraint.ConstraintLayout>


